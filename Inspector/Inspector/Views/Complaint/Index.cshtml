@model List<Complaint>


<div class="container">
	<div class="row pt-4 pb-4">
		<div class="col-6">
			<h2 class="text-primary">
				Complaint List
			</h2>
		</div>
		<div class="col-6 text-end">
			<a asp-controller="Complaint" asp-action="Create" class="btn btn-primary">
				<i class="bi bi-plus-lg"></i> Create Complaint
			</a>
		</div>
	</div>
	<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>
				Description
			</th>
			<th>
				OrganizationId
			</th>
			<th>
				UserName
			</th>
			<th>
				Status
			</th>
		</tr>
	</thead>
	<tbody>
			@foreach (var obj in Model)
			{
				<tr>
					<td>
						@obj.Description
					</td>
					<td>
						@obj.OrganizationId
					</td>
					<td>
						@obj.UserName
					</td>
					<td>
						@obj.Status
					</td>
				</tr>
			}
	</tbody>
</table>
</div>