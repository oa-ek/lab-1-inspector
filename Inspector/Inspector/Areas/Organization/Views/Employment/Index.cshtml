@model List<Employment>

<div class="container">
	<div class="row pt-4 pb-4">
		<div class="col-6">
			<h2 class="text-primary">
				Employement List
			</h2>
		</div>
	</div>
	<table id="tblData" class="table table-bordered table-striped" style="width:100%">
		<thead>
			<tr>
				<th>
					FullName
				</th>
				<th>
					Email
				</th>
				<th>

				</th>
			</tr>
		</thead>
</table>
</div>

@section Scripts {
	@{
		var randomParam = new Random().Next(10000);
	}
	<script src="~/js/organization/employment.js?cache=@randomParam"></script>
}